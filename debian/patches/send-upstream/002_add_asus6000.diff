--- xkb-data.orig/symbols/inet	2007-07-21 20:08:12.000000000 +0200
+++ xkb-data/symbols/inet	2007-07-21 20:08:12.000000000 +0200
@@ -229,6 +229,22 @@
     key <I74>   {       [ XF86Launch4           ]       };
 };
 
+// Asus
+
+// Asus A6000 series Keyboard
+partial alphanumeric_keys
+xkb_symbols "asus_6000" {
+  key <I20>   {       [ XF86AudioMute         ]       };
+  key <I2E>   {       [ XF86AudioLowerVolume  ]       };
+  key <I30>   {       [ XF86AudioRaiseVolume  ]       };
+  key <I6C>   {       [ XF86Mail              ]       };
+  key <I32>   {       [ XF86WWW               ]       };
+  key <I10>   {       [ XF86AudioPrev         ]       };
+  key <I19>   {       [ XF86AudioNext         ]       };
+  key <I24>   {       [ XF86AudioStop         ]       };
+  key <I22>   {       [ XF86AudioPlay         ]       };
+};
+
 // Azona
 
 // Azona RF2300 wireless Internet Keyboard
