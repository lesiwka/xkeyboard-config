-    key	<LatP> {	[     Cyrillic_pe,     Cyrillic_PE	]	};
+    key	<AD01> {	[     Cyrillic_ya,     Cyrillic_YA	]	};
+    key	<AB01> {	[     Cyrillic_ze,     Cyrillic_ZE	]	};
