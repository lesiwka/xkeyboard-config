+    key	<AC09> {	[     Cyrillic_el,     Cyrillic_EL	]	};
+    key	<AD10> {	[     Cyrillic_pe,     Cyrillic_PE	]	};
 
     key	<AD11> {	[    Cyrillic_sha,    Cyrillic_SHA	]	};
     key	<AE12> {	[    Cyrillic_che,    Cyrillic_CHE	]	};
