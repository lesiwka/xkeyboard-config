From 02408c1ed0aa0be97c827e642340b6d7f2a31cab Mon Sep 17 00:00:00 2001
From: Bryce Harrington <bryce@bryceharrington.org>
Date: Thu, 12 Feb 2009 21:28:45 -0800
Subject: [PATCH] ru: Fix shift key for Russian letters; include -/_ in Russian Phonetic layout

When entering capital Russian letters using Shift key, it prints lower
case symbol.  To fix, change "key <LatQ>..." to "key <AD01>...", etc.

-/_ is missing.  Add "key <AE11>..." for this.

Signed-off-by: Bryce Harrington <bryce@bryceharrington.org>
---
 symbols/ru |   53 +++++++++++++++++++++++++++--------------------------
 1 files changed, 27 insertions(+), 26 deletions(-)

diff --git a/symbols/ru b/symbols/ru
index dda7281..a6fa0e6 100644
--- a/symbols/ru
+++ b/symbols/ru
@@ -153,34 +153,35 @@ xkb_symbols "phonetic" {
     key	<AC10> {	[	semicolon,	     colon	]	};
     key	<AC11> {	[      apostrophe,	  quotedbl	]	};
     key	<LSGT> {	[	      bar,	 brokenbar	]	};
+    key	<AE11> {	[	    minus,	underscore	]	};
 
     key	<TLDE> {	[     Cyrillic_yu,     Cyrillic_YU	]	};
-    key	<LatQ> {	[     Cyrillic_ya,     Cyrillic_YA	]	};
-    key	<LatZ> {	[     Cyrillic_ze,     Cyrillic_ZE	]	};
-    key	<LatS> {	[     Cyrillic_es,     Cyrillic_ES	]	};
-    key	<LatA> {	[      Cyrillic_a,	Cyrillic_A	]	};
-    key	<LatW> {	[     Cyrillic_ve,     Cyrillic_VE	]	};
-    key	<LatC> {	[    Cyrillic_tse,    Cyrillic_TSE	]	};
-    key	<LatX> {	[Cyrillic_softsign,Cyrillic_SOFTSIGN	]	};
